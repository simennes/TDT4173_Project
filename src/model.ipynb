    "# open and unzip file to a pandas dataframe\n",
    "import zipfile\n",
    "import pandas as pd\n",
    "\n",
    "def open_zip(zip_file):\n",
    "    with zipfile.ZipFile(zip_file) as z:\n",
    "        for filename in z.namelist():\n",
    "            with z.open(filename) as f:\n",
    "                df = pd.read_csv(f)\n",
    "    return df\n"
   ]
  }
